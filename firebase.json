{
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
    "source": "functions"
  },
  "hosting": {
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "public": "public",
    "rewrites": [
      {
        "function": "amundi",
        "source": "/amundi"
      },
      {
        "function": "epsens",
        "source": "/epsens"
      }
    ]
  }
}
